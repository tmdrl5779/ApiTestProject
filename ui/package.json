{
  "name": "fe-jhw-react-boilerplate",
  "version": "1.0.0",
  "description": "react-boilerplate with webpack, babel, jest, lint, typescript",
  "main": "src/index.tsx",
  "packageManager": "yarn@3.3.0",
  "author": "fe-jhw",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.11.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.4",
    "mini-css-extract-plugin": "^2.7.0",
    "msw": "^0.49.1",
    "prettier": "^2.8.3",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.61.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "typescript": "4.4.2",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack server --config config/webpack/webpack.config.dev.js --open",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.config.prod.js",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "test": "jest --config config/jest/jest.config.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --fix"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
